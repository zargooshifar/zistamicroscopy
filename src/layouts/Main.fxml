<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="code.StageController">
    <children>
        <TabPane maxHeight="130.0" minHeight="130.0" prefHeight="130.0" stylesheets="@../assests/css/styles.css"
                 tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Stage Control">
                    <content>
                        <HBox alignment="CENTER_LEFT" maxHeight="100.0" minHeight="100.0" prefHeight="100.0"
                              style="-fx-background-color: #fff;">
                            <children>
                                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                    <children>
                                        <JFXButton prefWidth="100.0" text="Calibrate">
                                            <graphic>
                                                <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true"
                                                           preserveRatio="true">
                                                    <image>
                                                        <Image url="@../assests/icons/icons8_Ruler_16px.png"/>
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                            <VBox.margin>
                                                <Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
                                            </VBox.margin>
                                        </JFXButton>
                                        <HBox alignment="CENTER_LEFT" prefHeight="0.0" prefWidth="100.0">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Unit:"/>
                                                <Pane prefHeight="200.0" prefWidth="200.0"/>
                                                <JFXComboBox/>
                                            </children>
                                            <VBox.margin>
                                                <Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
                                            </VBox.margin>
                                        </HBox>
                                    </children>
                                    <HBox.margin>
                                        <Insets left="8.0" right="8.0"/>
                                    </HBox.margin>
                                </VBox>
                                <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                <VBox alignment="CENTER" spacing="4.0">
                                    <children>
                                        <HBox alignment="CENTER_LEFT" prefWidth="160.0">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Small Step Size:"
                                                      textOrigin="CENTER"/>
                                                <Pane HBox.hgrow="ALWAYS"/>
                                                <JFXTextField prefWidth="50.0" text="10" HBox.hgrow="NEVER"/>
                                            </children>
                                            <VBox.margin>
                                                <Insets/>
                                            </VBox.margin>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" prefWidth="160.0">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Medium Step Size:"
                                                      textOrigin="CENTER"/>
                                                <Pane HBox.hgrow="ALWAYS"/>
                                                <JFXTextField prefWidth="50.0" text="100" HBox.hgrow="NEVER"/>
                                            </children>
                                            <VBox.margin>
                                                <Insets/>
                                            </VBox.margin>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" prefWidth="160.0">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Large Step Size:"
                                                      textOrigin="CENTER"/>
                                                <Pane HBox.hgrow="ALWAYS"/>
                                                <JFXTextField prefWidth="50.0" text="1000" HBox.hgrow="NEVER"/>
                                            </children>
                                            <VBox.margin>
                                                <Insets/>
                                            </VBox.margin>
                                        </HBox>
                                    </children>
                                    <HBox.margin>
                                        <Insets left="8.0" right="8.0"/>
                                    </HBox.margin>
                                </VBox>
                                <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                <VBox alignment="CENTER" spacing="4.0">
                                    <children>
                                        <HBox alignment="CENTER_LEFT" prefWidth="160.0">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="X Speed:"
                                                      textOrigin="CENTER"/>
                                                <Pane HBox.hgrow="ALWAYS"/>
                                                <JFXTextField prefWidth="50.0" text="10" HBox.hgrow="NEVER"/>
                                            </children>
                                            <VBox.margin>
                                                <Insets/>
                                            </VBox.margin>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" prefWidth="160.0">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Y Speed:"
                                                      textOrigin="CENTER"/>
                                                <Pane HBox.hgrow="ALWAYS"/>
                                                <JFXTextField prefWidth="50.0" text="100" HBox.hgrow="NEVER"/>
                                            </children>
                                            <VBox.margin>
                                                <Insets/>
                                            </VBox.margin>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" prefWidth="160.0">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Z Speed"
                                                      textOrigin="CENTER"/>
                                                <Pane HBox.hgrow="ALWAYS"/>
                                                <JFXTextField prefWidth="50.0" text="1000" HBox.hgrow="NEVER"/>
                                            </children>
                                            <VBox.margin>
                                                <Insets/>
                                            </VBox.margin>
                                        </HBox>
                                    </children>
                                    <HBox.margin>
                                        <Insets left="8.0" right="8.0"/>
                                    </HBox.margin>
                                </VBox>
                                <Separator orientation="VERTICAL" prefHeight="200.0"/>
                            </children>
                            <padding>
                                <Insets bottom="4.0" top="4.0"/>
                            </padding>
                        </HBox>
                    </content>
                </Tab>
                <Tab text="Camera">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"
                                    style="-fx-background-color: #fff;">
                            <children>
                                <HBox alignment="CENTER_LEFT" prefHeight="103.0" prefWidth="900.0"
                                      style="-fx-background-color: #fff;">
                                    <children>
                                        <HBox alignment="CENTER" prefHeight="100.0" spacing="8.0">
                                            <children>
                                                <JFXButton fx:id="btn_capture" maxHeight="80.0" maxWidth="80.0"
                                                           minHeight="80.0" minWidth="80.0" onAction="#doCapture"
                                                           prefHeight="80.0" prefWidth="80.0" text="Capture"
                                                           textAlignment="CENTER" wrapText="true"/>
                                                <JFXButton fx:id="btn_capture_save" maxHeight="80.0" maxWidth="80.0"
                                                           minHeight="80.0" minWidth="80.0" onAction="#doCaptureSave"
                                                           prefHeight="80.0" prefWidth="80.0" text="Capture and Save"
                                                           textAlignment="CENTER" wrapText="true"/>
                                                <JFXButton fx:id="btn_startlive" maxHeight="80.0" maxWidth="80.0"
                                                           minHeight="80.0" minWidth="80.0"
                                                           onMouseClicked="#takeLiveStream" prefHeight="80.0"
                                                           prefWidth="80.0" text="Start Live Stream"
                                                           textAlignment="CENTER" wrapText="true"/>
                                            </children>
                                            <padding>
                                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                                            </padding>
                                        </HBox>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" spacing="8.0">
                                            <children>
                                                <HBox alignment="CENTER" fillHeight="false" prefWidth="200.0"
                                                      spacing="8.0">
                                                    <children>
                                                        <Text strokeType="OUTSIDE" strokeWidth="0.0"
                                                              text="Exposure time" HBox.hgrow="ALWAYS"/>
                                                        <Pane HBox.hgrow="ALWAYS"/>
                                                        <Spinner fx:id="exposure" prefWidth="70.0"/>
                                                    </children>
                                                </HBox>
                                                <HBox alignment="CENTER" prefWidth="200.0" spacing="8.0">
                                                    <children>
                                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Gain"
                                                              HBox.hgrow="ALWAYS"/>
                                                        <Pane HBox.hgrow="ALWAYS"/>
                                                        <Spinner fx:id="gain" prefWidth="70.0"/>
                                                    </children>
                                                </HBox>
                                            </children>
                                            <padding>
                                                <Insets left="16.0" right="16.0"/>
                                            </padding>
                                        </VBox>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" spacing="8.0">
                                            <children>
                                                <HBox alignment="CENTER" prefHeight="0.0" prefWidth="200.0"
                                                      spacing="8.0">
                                                    <children>
                                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="BitDepth"
                                                              HBox.hgrow="ALWAYS"/>
                                                        <Pane HBox.hgrow="ALWAYS"/>
                                                        <ChoiceBox fx:id="bitDepthPicker" maxWidth="80.0"
                                                                   minWidth="80.0" prefWidth="80.0"/>
                                                    </children>
                                                    <padding>
                                                        <Insets left="8.0" right="8.0"/>
                                                    </padding>
                                                </HBox>
                                                <HBox alignment="CENTER" prefHeight="0.0" prefWidth="200.0"
                                                      spacing="8.0">
                                                    <children>
                                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Pixel Type"
                                                              HBox.hgrow="ALWAYS"/>
                                                        <Pane HBox.hgrow="ALWAYS"/>
                                                        <ChoiceBox fx:id="pixelTypePicker" maxWidth="80.0"
                                                                   minWidth="80.0" prefWidth="80.0"/>
                                                    </children>
                                                    <padding>
                                                        <Insets left="8.0" right="8.0"/>
                                                    </padding>
                                                </HBox>
                                            </children>
                                            <padding>
                                                <Insets left="16.0" right="16.0"/>
                                            </padding>
                                        </VBox>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                    </children>
                                    <padding>
                                        <Insets bottom="4.0" top="4.0"/>
                                    </padding>
                                </HBox>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="View">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"
                                    style="-fx-background-color: #fff;">
                            <children>
                                <HBox alignment="CENTER_LEFT" prefHeight="103.0" prefWidth="900.0"
                                      style="-fx-background-color: #fff;">
                                    <children>
                                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                            <children>
                                                <JFXButton fx:id="viewScale" onAction="#changeViewScale"
                                                           prefWidth="100.0" text="Change View Scale">
                                                    <VBox.margin>
                                                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
                                                    </VBox.margin>
                                                </JFXButton>
                                            </children>
                                            <HBox.margin>
                                                <Insets left="8.0" right="8.0"/>
                                            </HBox.margin>
                                        </VBox>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="4.0">
                                            <children>
                                                <JFXButton onAction="#rotateLeft" text="RotateLeft"/>
                                                <JFXButton onAction="#rotateRight" text="RotateRight"/>
                                            </children>
                                        </VBox>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <JFXToggleButton fx:id="autoContrastToggle" contentDisplay="RIGHT"
                                                         text="Auto Contrast and Brightness" wrapText="true"/>
                                    </children>
                                    <padding>
                                        <Insets bottom="4.0" top="4.0"/>
                                    </padding>
                                </HBox>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Acquisition">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"
                                    style="-fx-background-color: #fff;">
                            <children>
                                <HBox alignment="CENTER_LEFT" prefHeight="103.0" prefWidth="900.0"
                                      style="-fx-background-color: #fff;">
                                    <children>
                                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                            <HBox.margin>
                                                <Insets left="8.0" right="8.0"/>
                                            </HBox.margin>
                                            <children>
                                                <HBox alignment="CENTER" prefHeight="25.0" prefWidth="179.0">
                                                    <children>
                                                        <JFXButton fx:id="startCapturingButton" alignment="CENTER"
                                                                   contentDisplay="CENTER" ellipsisString=""
                                                                   maxHeight="80.0" maxWidth="80.0" minHeight="80.0"
                                                                   minWidth="80.0" onAction="#startCapturing"
                                                                   prefHeight="80.0" prefWidth="80.0"
                                                                   text="Start Capturing" textAlignment="CENTER"
                                                                   wrapText="true"/>
                                                    </children>
                                                </HBox>
                                            </children>
                                        </VBox>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="160.0" spacing="6.0">
                                            <children>
                                                <HBox alignment="CENTER">
                                                    <children>
                                                        <Label ellipsisString="" maxWidth="1.7976931348623157E308"
                                                               text="Horizental Steps" HBox.hgrow="ALWAYS"/>
                                                        <JFXTextField fx:id="horizentalStepsInput" focusColor="#e4e8f2"
                                                                      prefHeight="25.0" prefWidth="55.0" text="100"
                                                                      HBox.hgrow="NEVER"/>
                                                    </children>
                                                </HBox>
                                                <HBox alignment="CENTER">
                                                    <children>
                                                        <Label ellipsisString="" maxWidth="1.7976931348623157E308"
                                                               text="Vertical Steps" HBox.hgrow="ALWAYS"/>
                                                        <JFXTextField fx:id="verticalStepsInput" focusColor="#e4e8f2"
                                                                      prefHeight="25.0" prefWidth="55.0" text="100"
                                                                      HBox.hgrow="NEVER"/>
                                                    </children>
                                                </HBox>
                                            </children>
                                            <HBox.margin>
                                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                                            </HBox.margin>
                                        </VBox>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="160.0" spacing="6.0">
                                            <children>
                                                <HBox alignment="CENTER">
                                                    <children>
                                                        <Label ellipsisString="" maxWidth="1.7976931348623157E308"
                                                               text="Start X" HBox.hgrow="ALWAYS"/>
                                                        <JFXTextField fx:id="XStartInput" focusColor="#e4e8f2"
                                                                      prefHeight="25.0" prefWidth="55.0" text="0"
                                                                      HBox.hgrow="NEVER"/>
                                                    </children>
                                                </HBox>
                                                <HBox alignment="CENTER">
                                                    <children>
                                                        <Label ellipsisString="" maxWidth="1.7976931348623157E308"
                                                               text="Start Y" HBox.hgrow="ALWAYS"/>
                                                        <JFXTextField fx:id="YStartInput" focusColor="#e4e8f2"
                                                                      prefHeight="25.0" prefWidth="55.0" text="0"
                                                                      HBox.hgrow="NEVER"/>
                                                    </children>
                                                </HBox>
                                                <JFXButton fx:id="setStartPointButton" onAction="#setStartPoint"
                                                           text="set Current Position">
                                                    <VBox.margin>
                                                        <Insets top="2.0"/>
                                                    </VBox.margin>
                                                </JFXButton>
                                            </children>
                                            <HBox.margin>
                                                <Insets left="16.0" right="16.0"/>
                                            </HBox.margin>
                                        </VBox>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="160.0" spacing="6.0">
                                            <children>
                                                <HBox alignment="CENTER">
                                                    <children>
                                                        <Label ellipsisString="" maxWidth="1.7976931348623157E308"
                                                               text="End X" HBox.hgrow="ALWAYS"/>
                                                        <JFXTextField fx:id="XEndInput" focusColor="#e4e8f2"
                                                                      prefHeight="25.0" prefWidth="55.0" text="1000"
                                                                      HBox.hgrow="NEVER"/>
                                                    </children>
                                                </HBox>
                                                <HBox alignment="CENTER">
                                                    <children>
                                                        <Label ellipsisString="" maxWidth="1.7976931348623157E308"
                                                               text="End Y" HBox.hgrow="ALWAYS"/>
                                                        <JFXTextField fx:id="YEndInput" focusColor="#e4e8f2"
                                                                      prefHeight="25.0" prefWidth="55.0" text="1000"
                                                                      HBox.hgrow="NEVER"/>
                                                    </children>
                                                </HBox>
                                                <JFXButton fx:id="setEndPointButton" onAction="#setEndPoint"
                                                           text="set Current Position">
                                                    <VBox.margin>
                                                        <Insets top="2.0"/>
                                                    </VBox.margin>
                                                </JFXButton>
                                            </children>
                                            <HBox.margin>
                                                <Insets left="16.0" right="16.0"/>
                                            </HBox.margin>
                                        </VBox>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                    </children>
                                    <padding>
                                        <Insets bottom="4.0" top="4.0"/>
                                    </padding>
                                </HBox>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Statistics">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"
                                    style="-fx-background-color: #fff;"/>
                    </content>
                </Tab>
                <Tab text="Configuration">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"
                                    style="-fx-background-color: #fff;"/>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <children>
                <VBox alignment="CENTER" maxWidth="200.0" minWidth="200.0" prefWidth="200.0"
                      style="-fx-background-color: #e3e3e3;">
                    <children>
                        <GridPane blendMode="EXCLUSION" maxHeight="180.0" maxWidth="180.0" minHeight="180.0"
                                  minWidth="180.0" prefHeight="180.0" prefWidth="180.0"
                                  style="-fx-background-color: #000; -fx-background-radius: 35px; -fx-border-color: #fff; -fx-border-radius: 45px;">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <JFXButton id="up" fx:id="upButton" ellipsisString="" maxHeight="1.7976931348623157E308"
                                           maxWidth="1.7976931348623157E308" onAction="#moveUpSmall"
                                           GridPane.columnIndex="1">
                                    <graphic>
                                        <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@../assests/icons/up.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </JFXButton>
                                <JFXButton id="left" fx:id="leftButton" ellipsisString=""
                                           maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                           onAction="#moveLeftSmall" GridPane.rowIndex="1">
                                    <graphic>
                                        <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@../assests/icons/left.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </JFXButton>
                                <JFXButton id="right" fx:id="rightButton" ellipsisString=""
                                           maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                           onAction="#moveRightSmall" GridPane.columnIndex="2" GridPane.rowIndex="1">
                                    <graphic>
                                        <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@../assests/icons/right.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </JFXButton>
                                <JFXButton id="bottom" fx:id="bottomButton" ellipsisString=""
                                           maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                           onAction="#moveBottomSmall" GridPane.columnIndex="1" GridPane.rowIndex="2">
                                    <graphic>
                                        <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@../assests/icons/bottom.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </JFXButton>
                                <JFXButton id="toggle" fx:id="toggleButton" ellipsisString=""
                                           maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                           onAction="#toggleStageButtons" GridPane.columnIndex="1"
                                           GridPane.rowIndex="1">
                                    <graphic>
                                        <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@../assests/icons/circle.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </JFXButton>
                                <JFXButton id="dup" fx:id="dupButton" ellipsisString="" layoutX="70.0" layoutY="10.0"
                                           maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                           onAction="#moveUpMedium" visible="false" GridPane.columnIndex="1">
                                    <graphic>
                                        <ImageView fx:id="dup" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@../assests/icons/dup.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </JFXButton>
                                <JFXButton id="dleft" fx:id="dleftButton" ellipsisString="" layoutX="10.0"
                                           layoutY="70.0" maxHeight="1.7976931348623157E308"
                                           maxWidth="1.7976931348623157E308" onAction="#moveLeftMedium" visible="false"
                                           GridPane.rowIndex="1">
                                    <graphic>
                                        <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@../assests/icons/dleft.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </JFXButton>
                                <JFXButton id="dright" fx:id="drightButton" ellipsisString="" layoutX="130.0"
                                           layoutY="70.0" maxHeight="1.7976931348623157E308"
                                           maxWidth="1.7976931348623157E308" onAction="#moveRightMedium" visible="false"
                                           GridPane.columnIndex="2" GridPane.rowIndex="1">
                                    <graphic>
                                        <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@../assests/icons/dright.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </JFXButton>
                                <JFXButton id="dbottom" fx:id="dbottomButton" ellipsisString="" layoutX="70.0"
