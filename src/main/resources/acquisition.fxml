<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER_LEFT" prefHeight="103.0" prefWidth="900.0"
         style="-fx-background-color: #fff;" xmlns="http://javafx.com/javafx/11.0.1" type="HBox">
    <children>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
            <HBox.margin>
                <Insets left="8.0" right="8.0"/>
            </HBox.margin>
            <children>
                <HBox alignment="CENTER" prefHeight="25.0" prefWidth="179.0">
                    <children>
                        <JFXButton fx:id="startCapturingButton" alignment="CENTER" contentDisplay="CENTER"
                                   ellipsisString="" maxHeight="80.0" maxWidth="80.0" minHeight="80.0" minWidth="80.0"
                                   onAction="#startCapturing" prefHeight="80.0" prefWidth="80.0" text="Start Capturing"
                                   textAlignment="CENTER" wrapText="true"/>
                    </children>
                </HBox>
            </children>
        </VBox>
        <Separator orientation="VERTICAL" prefHeight="200.0"/>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="160.0" spacing="6.0">
            <children>
                <HBox alignment="CENTER">
                    <children>
                        <Label ellipsisString="" maxWidth="1.7976931348623157E308" text="Horizental Steps"
                               HBox.hgrow="ALWAYS"/>
                        <JFXTextField fx:id="horizentalStepsInput" focusColor="#e4e8f2" prefHeight="25.0"
                                      prefWidth="55.0" text="100" HBox.hgrow="NEVER"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER">
                    <children>
                        <Label ellipsisString="" maxWidth="1.7976931348623157E308" text="Vertical Steps"
                               HBox.hgrow="ALWAYS"/>
                        <JFXTextField fx:id="verticalStepsInput" focusColor="#e4e8f2" prefHeight="25.0" prefWidth="55.0"
                                      text="100" HBox.hgrow="NEVER"/>
                    </children>
                </HBox>
            </children>
            <HBox.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </HBox.margin>
        </VBox>
        <Separator orientation="VERTICAL" prefHeight="200.0"/>
        <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="160.0" spacing="6.0">
            <children>
                <HBox alignment="CENTER">
                    <children>
                        <Label ellipsisString="" maxWidth="1.7976931348623157E308" text="Start X" HBox.hgrow="ALWAYS"/>
                        <JFXTextField fx:id="XStartInput" focusColor="#e4e8f2" prefHeight="25.0" prefWidth="55.0"
                                      text="0" HBox.hgrow="NEVER"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER">
                    <children>
                        <Label ellipsisString="" maxWidth="1.7976931348623157E308" text="Start Y" HBox.hgrow="ALWAYS"/>
                        <JFXTextField fx:id="YStartInput" focusColor="#e4e8f2" prefHeight="25.0" prefWidth="55.0"
                                      text="0" HBox.hgrow="NEVER"/>
                    </children>
                </HBox>
                <JFXButton fx:id="setStartPointButton" onAction="#setStartPoint" text="set Current Position">
                    <VBox.margin>
                        <Insets top="2.0"/>
                    </VBox.margin>
                </JFXButton>
            </children>
            <HBox.margin>
                <Insets left="16.0" right="16.0"/>
            </HBox.margin>
        </VBox>
        <Separator orientation="VERTICAL" prefHeight="200.0"/>
        <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="160.0" spacing="6.0">
            <children>
                <HBox alignment="CENTER">
                    <children>
                        <Label ellipsisString="" maxWidth="1.7976931348623157E308" text="End X" HBox.hgrow="ALWAYS"/>
                        <JFXTextField fx:id="XEndInput" focusColor="#e4e8f2" prefHeight="25.0" prefWidth="55.0"
                                      text="1000" HBox.hgrow="NEVER"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER">
                    <children>
                        <Label ellipsisString="" maxWidth="1.7976931348623157E308" text="End Y" HBox.hgrow="ALWAYS"/>
                        <JFXTextField fx:id="YEndInput" focusColor="#e4e8f2" prefHeight="25.0" prefWidth="55.0"
                                      text="1000" HBox.hgrow="NEVER"/>
                    </children>
                </HBox>
                <JFXButton fx:id="setEndPointButton" onAction="#setEndPoint" text="set Current Position">
                    <VBox.margin>
                        <Insets top="2.0"/>
                    </VBox.margin>
                </JFXButton>
            </children>
            <HBox.margin>
                <Insets left="16.0" right="16.0"/>
            </HBox.margin>
        </VBox>
        <Separator orientation="VERTICAL" prefHeight="200.0"/>
    </children>
    <padding>
        <Insets bottom="4.0" top="4.0"/>
    </padding>
</fx:root>
